def get_external_private_array(cust_vector, channel, player):
    """
        Function to read external private array from file.  Change in some of the
        original src/Input_Output/-Files of SCALE_MAMBA required.  For further
        information see SCALE-MAMBA documentation.
    """
    open_channel(channel)
    print_ln("channel no: %s", channel)
    condition = MemValue(cint(1))
    i = MemValue(cint(0))
    # mem_address = MemValue(0)
    sfloat_container_array = sint.Array(4)

    @do_while
    def _():
        @for_range(4)
        def _(j):
            sfloat_container_array[j] = sint.get_private_input_from(0,channel)

        input = sfloat(0)
        input.v = sfloat_container_array[0]
        input.p = sfloat_container_array[1]
        input.s = sfloat_container_array[2]
        input.z = sfloat_container_array[3]

        if_then((input.v != NULL).reveal())
        input.store_in_mem(MEM_ADDRESS)
        cust_vector.push_back(MEM_ADDRESS)
        i.write(i + 1)

        else_then()
        condition.write(0)
        end_if()
        return condition > 0

    close_channel(channel)

def get_random_index(data_length):
    '''
        Function to get a random index in the range of data_length. It is used
        within the split function.
    '''
    index = MemValue(sint(0))
    if_then((data_length < 16).reveal())
    index.write(MemValue(sint.get_random_int(4)))
    else_then()
    if_then((data_length < 32).reveal())
    index.write(MemValue(sint.get_random_int(5)))
    else_then()
    if_then((data_length < 64).reveal())
    index.write(MemValue(sint.get_random_int(6)))
    else_then()
    if_then((data_length < 128).reveal())
    index.write(MemValue(sint.get_random_int(7)))
    else_then()
    if_then((data_length < 256).reveal())
    index.write(MemValue(sint.get_random_int(8)))
    else_then()
    if_then((data_length < 512).reveal())
    index.write(MemValue(sint.get_random_int(9)))
    else_then()
    if_then((data_length < 1024).reveal())
    index.write(MemValue(sint.get_random_int(10)))
    else_then()
    if_then((data_length < 2048).reveal())
    index.write(MemValue(sint.get_random_int(11)))
    else_then()
    if_then((data_length < 4096).reveal())
    index.write(MemValue(sint.get_random_int(12)))
    else_then()
    if_then((data_length < 8192).reveal())
    index.write(MemValue(sint.get_random_int(13)))
    else_then()
    if_then((data_length < 16384).reveal())
    index.write(MemValue(sint.get_random_int(14)))
    else_then()
    if_then((data_length < 32768).reveal())
    index.write(MemValue(sint.get_random_int(15)))
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()
    end_if()

    return index

def split_up(x_vector, y_vector, split):
    """
        Function to split up dataset into training and test sets.

        Actual length of train and test (considering split) can not be taken into
        account at initialization, since array lengths have to be known at
        compile time.
    """
    x_train = sfloatCustVector()
    y_train = sfloatCustVector()
    x_test = sfloatCustVector()
    y_test = sfloatCustVector()
    data_length = x_vector.length()
    y_length = y_vector.length()

    if_then((data_length != y_vector.length()).reveal())
    print_ln("X and Y data do not have the same length! Check your input!")
    end_if()

    # Calculate target lengths of train and test vectors
    train_target_length = sfloat(data_length) * sfloat(split)
    train_target_length = mpc_math.floor_fx(sfix(train_target_length))
    test_target_length = data_length - train_target_length

    # Cast train_target_length to sint(), which is required in order to work
    # with the computation of the condition delta_check of the first while-loop.
    train_target_length_sint = floatingpoint.Trunc(
        train_target_length.v, train_target_length.k - train_target_length.f,
        train_target_length.f, train_target_length.kappa)
    train_target_length_sint_array = sint.Array(1)
    train_target_length_sint_array[0] = train_target_length_sint

    # Loop to add elements of data to train set. Elements are choosen randomly.
    # Way of getting random index to pick the elements should be improved,
    # if possible.  For now, since get_random_int() only takes python native int
    # as input, a second while loop is required.  It looks for a random int in
    # the range of MAX_ARRAY_LENGTH until this random number is within the range
    # of the data set.  This is very inefficient and slow.  A posisble solution
    # might involve changing the SCALE-MAMBA source code and might be
    # implemented later.
    @do_while
    def _():
        # TODO: make random faster with if checks
        index = MemValue(sint(0))
        index.write(get_random_index(data_length))
        #print_ln("index out ++++++++++++++++++++++++++++++++%s", index.reveal())

        condition = MemValue(1)
        x_element = sfloat.Array(1)
        y_element = sfloat.Array(1)

        # Use count for debugging
        count = MemValue(0)

        # Loop until random number within range of data_length found and
        # the element at this place has not been assigned to the train set, yet.
        @do_while
        def _():
            # if: 1st if-block
            if_then((index < data_length).reveal())
            # TODO: check for possibility to not have to reveal index here
            mem_address_get = x_vector.get_element(index.reveal())
            x_element[0] = sfloat.load_mem(mem_address_get)
            mem_address_get = y_vector.get_element(index.reveal())
            y_element[0] = sfloat.load_mem(mem_address_get)

            # if: 2nd if-block
            # TODO could add another check on y_element for safety reasons later
            if_then((x_element[0] != sfloat(NULL)).reveal())
            ind = index.reveal()
            null_element = sfloat(NULL)
            null_element.store_in_mem(MEM_ADDRESS)
            x_vector.set_element(ind, MEM_ADDRESS)
            y_vector.set_element(ind, MEM_ADDRESS)
            condition.write(0)

            # elseif: 2nd if-block
            else_then()
            # index.write(sint.get_random_int(8))
            index.write(get_random_index(data_length))
            # index.write(sint.get_random_int(nbit))
            count.write(count + 1)

            # end: 2nd if-block
            end_if()

            # elseif: 1st if-block
            else_then()
            # index.write(sint.get_random_int(8))
            # index.write(sint.get_random_int(nbit))
            index.write(get_random_index(data_length))
            count.write(count + 1)

            # end: 1st if-block
            end_if()
            return condition > 0

        #print_ln("count: %s", count)
        #print_ln("index: %s",index.reveal())
        #print_ln("x_elem: %s", x_element[0].reveal())
        #print_ln("y_elem: %s", y_element[0].reveal())

        x_element[0].store_in_mem(MEM_ADDRESS)
        x_train.push_back(MEM_ADDRESS)
        y_element[0].store_in_mem(MEM_ADDRESS)
        y_train.push_back(MEM_ADDRESS)
        delta_check = (train_target_length_sint_array[0]
            - x_train.length()).reveal()
        #print_ln("progress splitting: %s", delta_check)
        return delta_check > 0

    # Loop over data vector to assign all left elements in data to test vector.
    @for_range(data_length.reveal())
    def _(i):
        mem_address_get = x_vector.get_element(cint(i))
        x_element = sfloat.load_mem(mem_address_get)
        mem_address_get = y_vector.get_element(cint(i))
        y_element = sfloat.load_mem(mem_address_get)

        if_then((x_element != sfloat(NULL)).reveal())
        if_then((y_element != sfloat(NULL)).reveal())
        x_element.store_in_mem(MEM_ADDRESS)
        x_test.push_back(MEM_ADDRESS)
        y_element.store_in_mem(MEM_ADDRESS)
        y_test.push_back(MEM_ADDRESS)
        end_if()
        end_if()

    # print_ln("==============================")
    # x_train.show(25)
    # print_ln("==============================")
    # x_test.show(25)
    # print_ln("==============================")
    # y_train.show(25)
    # print_ln("==============================")
    # y_test.show(25)

    return x_train, x_test, y_train, y_test

